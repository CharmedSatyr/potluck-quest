{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": [".next/**", "!.next/cache/**", "dist/**"]
      },
      "test": {
        "dependsOn": ["^test"]
      },
      "dev": {
        "persistent": true,
        "cache": false
      }
    }
  }